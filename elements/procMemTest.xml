<?xml version="1.0"?>

<config>
    stopAtCycle=1000000000000
    run-mode=both
</config>


<sst>
	<component id="genericProc" >	
		<genericProc>
			<params>
              <debug>0</debug>
			  <clock>2.0Ghz</clock>
			  <execFile>components/genericProc/programs/fooPrint</execFile>
			  <verbose>0</verbose>
			  <externalMem>1</externalMem>
			 </params>
			<links>
				<link id="cpu2mem">
					<params>
						<name>mem0</name>
						<lat>1ns</lat>
					</params>
				</link>
			</links>
		</genericProc>
	</component>

 	<component id="mem0">
 		<DRAMSimC>
 			<params>
                 <clock>2.0Ghz</clock>
                 <systemini>system.ini</systemini>
                 <deviceini>ini/DDR3_micron_32M_8B_x8_sg15.ini</deviceini>
                 <pwd>DRAMSim</pwd>
                 <printstats>no</printstats>
 			</params>
 			<links>
 				<link id="cpu2mem">
 					<params>
 						<name>bus</name>
 						<lat>1ns</lat>
 					</params>
 				</link>
 			</links>
 		</DRAMSimC>
 	</component>

</sst>
