<?xml version="1.0"?>
<config>
    stopAtCycle=1000000
</config>
<sst>
	<component id="trace" >	
		<DRAMSimTrace>
			<params> 
                <clock>2.2GHz</clock>
                <tracefile>mase_gcc.trc</tracefile>
                <pwd>DRAMSim</pwd>
            </params>
			<links>
				<link id="link">
					 <params>
                        <lat>1ns</lat>
                        <name>bus</name>
					 </params>
				</link>
			</links>
		</DRAMSimTrace>
	</component>

	<component id="dramsimx">	
		<DRAMSimC>
			<params>
                <clock>2.2Ghz</clock>
                <systemini>system.ini</systemini>
                <deviceini>ini/DDR3_micron_32M_8B_x8_sg15.ini</deviceini>
                <pwd>DRAMSim</pwd>
                <printstats>no</printstats>
			</params>
			<links>
				<link id="link">
					 <params>
                        <lat>1ns</lat>
                        <name>bus</name>
					 </params>
				</link>
			</links>
		</DRAMSimC>
	</component>

</sst>
